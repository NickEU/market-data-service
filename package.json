{
  "name": "arch-capstone",
  "version": "1.0.0",
  "description": "API for educational project",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/src/main.js",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
    "generate": "prisma generate",
    "build": "tsc",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "test:clinic": "clinic doctor --on-port 'autocannon -m POST localhost:8000/users/register' -- node dist/main.js",
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.ts --coverage"
  },
  "author": "Nick Knock",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "@types/coingecko-api": "^1.0.10",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "coingecko-api": "^1.0.10",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "got": "^11.8.3",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.2.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "prisma": "^3.11.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
